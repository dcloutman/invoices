<!DOCTYPE html>
<html>
<head>
    {% block docHead %}
    <title>{% block docTitle %}{% endblock %}</title>
    {% endblock %}
    {% block favicons %}
    <link rel="apple-touch-icon" sizes="180x180" href="{{ url_for('static', filename='favicons/apple-touch-icon.png') }}" />
    <link rel="icon" type="image/png" sizes="32x32" href="{{ url_for('static', filename='favicons/favicon-32x32.png') }}" />
    <link rel="icon" type="image/png" sizes="16x16" href="{{ url_for('static', filename='favicons/favicon-16x16.png') }}" />
    {% endblock %}
</head>
<body>

<div id="outer_container">
    <header id="page_header">
        {% block pageHeader %}{% endblock %}
    </header>
    <main id="page_interface">{% block pageInterface %}{% endblock %}</main>
    <footer>
        &#169; David Cloutman All rights reserved.
    </footer>
</div>

{% block bodyScripts %}
{% endblock %}

</body>
</html>
